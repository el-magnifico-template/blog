  var group_input = $(this);
  if (group_input.length) {
    group_input.appendTo($("#before-ad"));
  }
});
$("#main-after-ad .widget").each(function() {
  var group_input = $(this);
  if (group_input.length) {
    group_input.appendTo($("#after-ad"));
  }
});



