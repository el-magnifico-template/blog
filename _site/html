<script async='async'>
//  <![CDATA[ 
var id = document.getElementById('post-body') ;

// lazy load image
paragraph = id.querySelectorAll('img');
	
	for(var count = 0; count < paragraph.length; count++)
	{
	
	if (paragraph[count].alt == '' )	  {
		
		paragraph[count].setAttribute("alt", paragraph[count].src.substring((paragraph[count].src.lastIndexOf('/'))+1, paragraph[count].src.lastIndexOf('.')));		
	
 }
if (paragraph[count].title == '' )	  {
		
		paragraph[count].setAttribute("title", paragraph[count].src.substring((paragraph[count].src.lastIndexOf('/'))+1, paragraph[count].src.lastIndexOf('.')));		

 }
	if (!paragraph[count].hasAttribute("data-src")) {
		paragraph[count].setAttribute("data-src", paragraph[count].src/*.replace('.jpg','html')*/);
        paragraph[count].removeAttribute("src");
		
 }





 }
 
 // lazy load iframe
/*
 paragraph = id.querySelectorAll('iframe');
	
	for(var count = 0; count < paragraph.length; count++)
	{
	if (!paragraph[count].hasAttribute("data-src"))
	{
		paragraph[count].setAttribute("data-src", paragraph[count].src );
		paragraph[count].setAttribute("class", "lazy" );

        paragraph[count].removeAttribute("src");
		
 } } 
*/
//]]>

 
// nofollow and _blank
var sss ="";
var hpajj =  "https://el-magnifico-template.blogspot.com/";
hpajj = hpajj.replace("https://", "");
 //<![CDATA[



  var anchors = id.querySelectorAll('a');
for (var i = 0; i < anchors.length; i++) {
sss = anchors[i].getAttribute("href") ;
// TODO
if (sss  != null )	  {
if (sss.indexOf(hpajj) == -1  )	  {
anchors[i].setAttribute('target', '_blank');
anchors[i].setAttribute("rel","nofollow");
 }
 }
 }

 // internal backlink
for (var j = 0; j < labels.length; j++) {
highlightWord(id,labels[j]);
}
function highlightWord(root,word){
  textNodesUnder(root).forEach(highlightWords);

  function textNodesUnder(root){
    var walk=document.createTreeWalker(root,NodeFilter.SHOW_TEXT,null,false),
        text=[], node;
    while(node=walk.nextNode()) text.push(node);
    return text;
  }
    
  function highlightWords(n){
    for (var i; (i=n.nodeValue.indexOf(word,i)) > -1; n=after){
      var after = n.splitText(i+word.length);
	  	if (after.parentNode.nodeName != 'A') {

      var highlighted = n.splitText(i);
      var span = document.createElement('a');
	  span.href = "/search/label/"+word ;
      span.className = 'highlight';
      span.appendChild(highlighted);
	span.innerHTML = ' # ' + span.innerHTML ;
	after.parentNode.insertBefore(span,after);	
	}
    }
  }
}


// read also links
 
   if (read_also_post) {
    var spreadsheets = document.querySelectorAll(".post-body br, .post-body p ,.post-body span , .post-body div");
    var i = parseInt(spreadsheets.length / 2);
   var new_node = document.createElement("div");
  // new_node.className="alert-message";
    new_node.innerHTML = "<b>"+read_also+":</b>"+read_also_post;
    spreadsheets[i].parentNode.insertBefore(new_node, spreadsheets[i].nextSibling);
     
  }
 




// toc
        
(function(f){var h=function(k,j){for(var l in j){if(j.hasOwnProperty(l)&&j[l]){k[l]=j[l]}}return k};var i=function(j,l){var k=[];var m=document.querySelectorAll(l);Array.prototype.forEach.call(m,function(o){var n=o.querySelectorAll(j);k=k.concat(Array.prototype.slice.call(n))});return k};var g=function(k){if(typeof k!=="string"){return 0}var j=k.match(/\d/g);return j?Math.min.apply(null,j):1};var c=function(l,k){while(k--){var j=document.createElement("ol");l.appendChild(j);l=j}return l};var b=function(j,k){if(k<0){return b(j.parentElement,k+1)}if(k>0){return c(j,k)}return j.parentElement};var d=function(j,k){return function(p,o,l){var m=p.textContent;var n=k+"-"+l;o.textContent=p.textContent;var q=j?n:(p.id||n);q=encodeURIComponent(q);p.id=q;o.href="#"+q}};var e=function(l){var j=l.selector;var m=l.scope;var k=document.createElement("ol");var o=k;var n=d(l.overwrite,l.prefix);i(j,m).reduce(function(s,v,r){var t=g(v.tagName);var u=t-s;var w=b(o,u)||k;var p=document.createElement("li");var q=document.createElement("a");n(v,q,r);w.appendChild(p).appendChild(q);o=p;return t},g(j));return k};var a=function(l){var k={selector:"h1, h2, h3, h4, h5, h6",scope:"body",overwrite:false,prefix:"toc"};l=h(k,l);var j=l.selector;if(typeof j!=="string"){throw new TypeError("selector must be a string")}if(!j.match(/^(?:h[1-6],?\s*)*$/g)){throw new TypeError("selector must contains only h1-6")}var m=location.hash;if(m){setTimeout(function(){location.hash="";location.hash=m},0)}return e(l)};if(typeof define==="function"&&define.amd){define("toc",[],function(){return a})}else{f.initTOC=a}}(window));	
           

        var aside = document.getElementById('toc');
        var toc = initTOC({
            selector: 'h2',
            scope: '.post-body'
        });
        aside.appendChild(toc);
		  if ( toc.querySelector("li") != null) {
	  document.getElementById('toc_container').style.display = 'block';
	    }	  

