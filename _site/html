  var filedata ;
var filename = /* "firstfile.txt" */ "posts/"+fileName;
            var filemessage = /* "uploading a file" */ fileName;
            var filecontent = /* "The data of the file2." */ 10 ;
        var basecontent = btoa(filecontent);
        var apiurl = "https://api.github.com/repos/"+ github_un+"/"+github_un+".github.io" +"/contents/{+path}".replace('{+path}',filename);
 filedata = '{"message":"'+filemessage+'","content":"'+basecontent+'"}';

$.getJSON( apiurl , function(data) {
  sha = data.sha ;
  views = atob( data.content );
    intview = parseInt(views, 10);

basecontent = btoa(parseInt(views, 10) + Math.floor(Math.random() * 3 ) + 1);
filedata = '{"message":"'+filemessage+'","content":"'+basecontent+'", "sha":"'+sha+'" }';

}).always(function(response) {
 document.getElementById("entry-view").innerHTML = intview+' ' +  view ;
   $.ajax({ 
            url: apiurl,
            type: 'PUT',
            beforeSend: function(xhr){xhr.setRequestHeader("Authorization", "Basic " + btoa(github_un+":"+github_pw)); },
            data: filedata
        }).done(function(response) {
            console.log(response);	
        });       
}).fail(function(response) {
if (response.status === 404 && up_date > 201908310000) {
notifi();
 
 }   
});
 }
 
