  if (message.match("//img1.blogblog.com/img/blank.gif")) {
    var ongoingMessage = "//4.bp.blogspot.com/-oSjP8F09qxo/Wy1J9dp7b0I/AAAAAAAACF0/ggcRfLCFQ9s2SSaeL9BFSE2wyTYzQaTyQCK4BGAYYCw/w55-h55-p-k-no-nu/avatar.jpg";
  } else {
    ongoingMessage = message;
  }
   var _0xd46dx40 = '<article class="custom-item item-' + id + '"><a class="entry-image-link cmm-avatar" href="' + post_id + '"><img class="entry-thumb" data-src="' + ongoingMessage + '"/></a><h2 class="entry-title"><a href="' + post_id + '">' + secondary + '</a></h2><p class="cmm-snippet excerpt">' + video1title + "</p></article>";
  return _0xd46dx40;
}
function getCustomStyle(state, module, object) {
  if (object != "") {
    if (state == "featured") {
      var superStyle = ".id-" + state + "-" + module + " .entry-category{background-color:" + object + ";color:#fff}.id-" + state + "-" + module + " .loader:after{border-color:" + object + ";border-right-color:rgba(155,155,155,0.2)}";
    } else {
      superStyle = ".id-" + state + "-" + module + " .title-wrap:after,.id-" + state + "-" + module + " .entry-category{background-color:" + object + ";color:#fff}.id-" + state + "-" + module + " .title-wrap > a.more:hover,.id-" + state + "-" + module + " .entry-header:not(.entry-info) .entry-title a:hover{color:" + object + "}.id-" + state + "-" + module + " .loader:after{border-color:" + object + ";border-right-color:rgba(155,155,155,0.2)}";
    }
  } else {
    superStyle = "";
  }
  return superStyle;
}
function getAjax(table, type, method, name, callback) {
  switch(type) {
    case "msimple":
    case "featured":
    case "block1":
    case "col-left":
    case "col-right":
    case "grid1":
    case "videos":
    case "list":
    case "related":
      if (name == undefined) {
        name = "geterror404";
      }
      var url = getFeedUrl(type, method, name);
      $.ajax({
        url : url,
        type : "GET",
        dataType : "json",
        cache : true,
        beforeSend : function(xhr) {
          var element = getCustomStyle(type, name, callback);
          switch(type) {
            case "featured":
              $("#page-skin-2").prepend(element);
              table.html(beforeLoader()).parent().addClass("id-" + type + "-" + name + " show-ify");
              break;
            case "block1":
            case "grid1":
            case "videos":
              $("#page-skin-2").prepend(element);
              table.html(beforeLoader()).parent().addClass("id-" + type + "-" + name + " show-ify");
              break;
            case "col-left":
              $("#page-skin-2").prepend(element);
              table.html(beforeLoader()).parent().addClass("column-left block-column id-" + type + "-" + name + " show-ify");
              break;
            case "col-right":
              $("#page-skin-2").prepend(element);
              table.html(beforeLoader()).parent().addClass("column-right block-column id-" + type + "-" + name + " show-ify");
              break;
            case "list":
              table.html(beforeLoader());
              break;
            case "related":
              table.html(beforeLoader()).parent().addClass("show-ify");
              break;
          }
        },
        success : function(data) {
          var value = "";
          switch(type) {
            case "msimple":
              value = '<ul class="mega-widget">';
              break;
            case "featured":
              value = '<div class="featured-posts">';
              break;
            case "block1":
              value = '<div class="block-posts-1">';
              break;
            case "col-left":
            case "col-right":
              value = '<div class="column-posts">';
              break;
            case "grid1":
              value = '<div class="grid-posts-1">';
              break;
            case "videos":
              value = '<div class="block-videos">';
              break;
            case "list":
              value = '<div class="custom-widget">';
              break;
            case "related":
              value = '<div class="related-posts">';
              break;
          }
          var k = data.feed.entry;
          if (k != undefined) {
            var i = 0;
            var id = k;
            for (; i < id.length; i++) {
              var url = getPostLink(id, i);
              var key = getPostTitle(id, i, url);
              var filePath = getPostImage(id, i, url);
              var personId = getPostAuthor(id, i);
              var date = getPostDate(id, i);
              var p = getPostMeta(personId, date);
              var thisM = getPostLabel(id, i);
              var s = "";
              switch(type) {
                case "msimple":
                  s = s + ('<article class="mega-item"><div class="mega-content"><a class="entry-image-link" href="' + url + '"><img  title="' + key + '" alt="' + key + '" class="entry-thumb" data-src="' + filePath + '"/></a><h2 class="entry-title"><a href="' + url + '">' + key + "</a></h2>" + p[1] + "</div></article>");
                  break;
                case "featured":
                  switch(i) {
                    case 0:
                    case 1:
                      s = s + ('<article class="featured-item post item-' + i + '"><div class="featured-item-inner"><a class="entry-image-link before-mask" href="' + url + '"><img title="' + key + '" alt="' + key + '" class="entry-thumb" data-src="' + filePath + '"/></a>' + thisM + '<div class="entry-header entry-info"><h2 class="entry-title"><a href="' + url + '">' + key + "</a></h2>" + p[0] + "</div></div></article>");
                      break;
                    default:
                      s = s + ('<article class="featured-item post item-' + i + '"><div class="featured-item-inner"><a class="entry-image-link before-mask" href="' + url + '"><img title="' + key + '" alt="' + key + '" class="entry-thumb" data-src="' + filePath + '"/></a>' + thisM + '<div class="entry-header entry-info"><h2 class="entry-title"><a href="' + url + '">' + key + "</a></h2>" + p[1] + "</div></div></article>");
                      break;
                  }break;
                case "block1":
                  switch(i) {
                    case 0:
                      s = s + ('<article class="block-item item-' + i + '"><div class="block-inner">' + thisM + '<a class="entry-image-link before-mask" href="' + url + '"><img title="' + key + '" alt="' + key + '" class="entry-thumb" data-src="' + filePath + '"/></a><div class="entry-header entry-info"><h2 class="entry-title"><a href="' + url + '">' + key + "</a></h2>" + p[0] + "</div></div></article>");
                      break;
                    default:
                      s = s + ('<article class="block-item item-' + i + '"><a class="entry-image-link" href="' + url + '"><img  title="' + key + '" alt="' + key + '" class="entry-thumb" data-src="' + filePath + '"/></a><div class="entry-header"><h2 class="entry-title"><a href="' + url + '">' + key + "</a></h2>" + p[1] + "</div></article>");
                      break;
                  }break;
                case "col-left":
                case "col-right":
                  switch(i) {
                    case 0:
                      s = s + ('<article class="column-item item-' + i + '"><div class="column-inner">' + thisM + '<a class="entry-image-link before-mask" href="' + url + '"><img title="' + key + '"  alt="' + key + '" class="entry-thumb" data-src="' + filePath + '"/></a><div class="entry-header entry-info"><h2 class="entry-title"><a href="' + url + '">' + key + "</a></h2>" + p[0] + "</div></div></article>");
                      break;
                    default:
                      s = s + ('<article class="column-item item-' + i + '"><a class="entry-image-link" href="' + url + '"><img title="' + key + '" alt="' + key + '" class="entry-thumb" data-src="' + filePath + '"/></a><div class="entry-header"><h2 class="entry-title"><a href="' + url + '">' + key + "</a></h2>" + p[1] + "</div></article>");
                      break;
                  }break;
                case "grid1":
                  s = s + ('<article class="grid-item item-' + i + '"><div class="entry-image"><a class="entry-image-link" href="' + url + '"><img title="' + key + '" alt="' + key + '" class="entry-thumb" data-src="' + filePath + '"/></a></div><div class="entry-header"><h2 class="entry-title"><a href="' + url + '">' + key + "</a></h2>" + p[1] + "</div></article>");
                  break;
                case "videos":
                  s = s + ('<article class="videos-item item-' + i + '"><div class="entry-image"><a class="entry-image-link" href="' + url + '"><img title="' + key + '" alt="' + key + '" class="entry-thumb" data-src="' + filePath + '"/><span class="video-icon"/></a></div><div class="entry-header"><h2 class="entry-title"><a href="' + url + '">' + key + "</a></h2>" + p[1] + "</div></article>");
                  break;
                case "list":
								 

                  switch(name) {
				
				  
                    case "comments":
				 
                     
                      s = s + getPostComments(id, i, url);
                      break;
                    default:
                      s = s + ('<article class="custom-item item-' + i + '"><a class="entry-image-link" href="' + url + '"><img title="' + key + '" alt="' + key + '" class="entry-thumb" data-src="' + filePath + '"/></a><div class="entry-header"><h2 class="entry-title"><a href="' + url + '">' + key + "</a></h2>" + p[1] + "</div></article>");
                      break;
                  }break;
                case "related":
                  s = s + ('<article class="related-item post item-' + i + '"><div class="entry-image"><a class="entry-image-link" href="' + url + '"><img  title="' + key + '" alt="' + key + '" class="entry-thumb" data-src="' + filePath + '"/></a></div><div class="entry-header"><h2 class="entry-title"><a href="' + url + '">' + key + "</a></h2>" + p[1] + "</div></article>");
                  break;
              }
              value = value + s;
            }
          } else {
            switch(type) {
              case "msimple":
                value = '<ul class="mega-widget">' + msgError() + "</ul>";
                break;
              default:
                value = msgError();
                break;
            }
          }
          switch(type) {
            case "msimple":
              value = value + "</ul>";
              table.append(value).addClass("msimple");
              table.find("a:first").attr("href", function(canCreateDiscussions, iframe) {
                switch(name) {
                  case "recent":
                    iframe = iframe.replace(iframe, "/search");
                    break;
                  default:
                    iframe = iframe.replace(iframe, "/search/label/" + name);
                    break;
                }
                return iframe;
              });
              break;
            case "block1":
            case "grid1":
            case "col-left":
            case "col-right":
            case "videos":
              value = value + "</div>";
              table.html(value);
              break;
            default:
              value = value + "</div>";
              table.html(value);
              break;
          }
      if (lazyLoadInstance) {
    lazyLoadInstance.update();
}
     var imgs = document.getElementsByTagName("img");

  for (var i = 0; i < imgs.length; i++) {
	  
     
	  if (imgs[i].hasAttribute("data-src") && !imgs[i].hasAttribute("src") ){
	   var img = imgs[i].getAttribute("data-src");
      var height =parseInt(getComputedStyle(imgs[i], null).height.replace("px", ""))

// 	  Math.round(imgs[i].offsetWidth);
	  
      var x = parseInt(getComputedStyle(imgs[i], null).width.replace("px", ""))
 // Math.round(imgs[i].offsetHeight);
      var width = "/w" + x + "-h" + height + "-p-k-no-nu";
      if (img.match("/s72-c")) {
        img = img.replace("/s72-c", width);
      } else {
        if (img.match("/w72-h72-p-k-no-nu")) {
          img = img.replace("/w72-h72-p-k-no-nu", width);
        } else {
          img = img;
        }
      }
	  
	 imgs[i].setAttribute("data-src", img ); 
}
}

    },
        error : function() {
          switch(type) {
            case "msimple":
              table.append("<ul>" + msgServerError() + "</ul>");
              break;
            default:
              table.html(msgServerError());
              break;
          }
        }
      });
  }
}

function ajaxFeatured(table, user, other_user, next, searchStr, type) {
  if (searchStr.match("getfeatured")) {
    if (user == "featured") {
      return getAjax(table, user, other_user, next, type);
    } else {
      table.html(beforeLoader()).parent().addClass("show-ify");
      setTimeout(function() {
        table.html(msgError());
      }, 500);
    }
  }
}
function ajaxBlock(table, type, key, last, collectionName, cb) {
  if (collectionName.match("getblock")) {
    if (type == "block1" || type == "col-left" || type == "col-right" || type == "grid1" || type == "videos") {

     var   msg = messages.showMore;
      
      table.parent().find(".widget-title").append('<a class="more" href="/search/label/' + last + '">' + msg + "</a>");
      return getAjax(table, type, key, last, cb);
    } else {
      table.html(msgError()).parent().addClass("show-ify");
    }
  }
}
function ajaxWidget(table, key, type, values, name) {
  if (name.match("getwidget")) {
    if (key == "list") {
      return getAjax(table, key, type, values);
    } else {
      table.html(msgError());
    }
  }
}
function ajaxRelated(table, object, key, name, value) {
  if (value.match("getrelated")) {
    return getAjax(table, object, key, name);
  }
}

