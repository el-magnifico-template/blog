     var imgs = document.getElementsByTagName("img");

  for (var i = 0; i < imgs.length; i++) {
	  
     
	  if (imgs[i].hasAttribute("data-src") && !imgs[i].hasAttribute("src") ){
	   var img = imgs[i].getAttribute("data-src");
      var x = Math.round(imgs[i].offsetWidth);
      var height = Math.round(imgs[i].offsetHeight);
      var width = "/w" + x + "-h" + height + "-p-k-no-nu";
      if (img.match("/s72-c")) {
        img = img.replace("/s72-c", width);
      } else {
        if (img.match("/w72-h72-p-k-no-nu")) {
          img = img.replace("/w72-h72-p-k-no-nu", width);
        } else {
          img = img;
        }
      }
	  
	 imgs[i].setAttribute("data-src", img ); 
}
}				  
				  
 


				  
				  
				  
