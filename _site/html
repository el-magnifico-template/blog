    $obfuscatedEmail.replaceWith('<div id="new-before-ad"/>');
  }
  if (_0xd46dx12 == "$ads={2}") {
    $obfuscatedEmail.replaceWith('<div id="new-after-ad"/>');
  }
});
$("#new-before-ad").each(function() {
  var $pluginHolder = $(this);
  if ($pluginHolder.length) {
    $("#before-ad").appendTo($pluginHolder);
  }
});
$("#new-after-ad").each(function() {
  var $pluginHolder = $(this);
  if ($pluginHolder.length) {
    $("#after-ad").appendTo($pluginHolder);
  }
});
