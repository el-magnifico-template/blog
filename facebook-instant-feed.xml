			<meta property="og:type"         content="article" />
			<meta property="og:title"        content="The first mass-produced book to deviate from a rectilinear format" />
			<meta property="og:description"  content="" />
			<meta property="og:image"        content="https://tellmemore.life/images/cardboard.jpg" />
			<meta property="fb:app_id"       content="" />
